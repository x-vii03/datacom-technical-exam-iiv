<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Datacom WebPage Test" verbose="1">
    <test name="Datacom - Our locations page - New Zealand">
        <parameter name="header" value="Our locations"/>
        <parameter name="defaultContext"
                   value="Contact one of our global offices or one of our teams to find out more about how we can help you, or to answer any query you may have."/>
        <parameter name="tabNames" value="Our locations,Get in touch,Media pack"/>
        <parameter name="regions" value="New Zealand,Australia,Asia"/>
        <parameter name="region" value="New Zealand"/>
        <parameter name="location" value="Nelson"/>
        <classes>

            <class name="com.datacom.tests.OurLocationsPageTest"/>
        </classes>
    </test>

    <test name="Datacom - Our locations page - Australia">
        <parameter name="header" value="Our locations"/>
        <parameter name="defaultContext"
                   value="Contact one of our global offices or one of our teams to find out more about how we can help you, or to answer any query you may have."/>
        <parameter name="tabNames" value="Our locations,Get in touch,Media pack"/>
        <parameter name="regions" value="New Zealand,Australia,Asia"/>
        <parameter name="region" value="Australia"/>
        <parameter name="location" value="Modbury"/>
        <classes>

            <class name="com.datacom.tests.OurLocationsPageTest"/>
        </classes>
    </test>

</suite>